{
  "title": "Temperature",
  "abstract": "Temperature",
  "producer": "SMARTMET",
  "projection": {
    "crs": "data",
    "size": 1.0
  },
  "views": [
    {
      "layers": [
        {
          "qid": "l1",
          "layer_type": "isoband",
          "parameter": "T-K",
          "isobands": "json:isobands/T-K.json",
          "css": "isobands/temperature.css",
          "smoother": {
            "degree": 1,
            "size": 1
          }
        },
        {
          "qid": "l2",
          "layer_type": "number",
          "css": "numbers/integers.css",
          "parameter": "T-K",
          "attributes": {
            "class": "Number",
            "id": "wind_numbers"
          },
          "label": "json:numbers/integers.json",
          "positions": {
            "x": 15,
            "y": 15,
            "dx": 20,
            "dy": 20,
            "ddx": 10
          }
        },
        {
          "qid": "l3-",
          "layer_type": "map",
          "map": {
            "lines": true,
            "schema": "esri",
            "table": "europe_country_wgs84"
          },
          "attributes": {
            "id": "world_country_line",
            "class": "Border",
            "fill": "none",
            "stroke": "#222",
            "stroke-width": "0.5px"
          }
        },
        {
          "qid": "l11",
          "tag": "rect",
          "attributes": {
            "x": -168,
            "y": 3,
            "width": 155,
            "height": 25,
            "rx": 8,
            "ry": 8,
            "fill": "white",
            "stroke": "black",
            "stroke-width": "0.6"
          }
        },
        {
          "qid": "l12",
          "layer_type": "time",
          "timestamp": "validtime",
          "timezone": "Europe/Helsinki",
          "format": "%Y-%m-%d %H:%M",
          "x": -20,
          "y": 20,
          "attributes": {
            "text-anchor": "end",
            "font-family": "Verdana",
            "font-weight": "bold",
            "font-size": 14
          }
        }        
      ]
    }
  ]
}
